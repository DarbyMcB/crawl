<!DOCTYPE HTML>
<html>
  <head>
    <title>WebTiles - Dungeon Crawl Stone Soup</title>
    <link rel="icon" href="/static/stone_soup_icon-32x32.png" type="image/png">
    <link rel="stylesheet" type="text/css" href="/static/style.css">
    <link rel="stylesheet" type="text/css" href="/static/simplebar.css">
  </head>
  <body>
    <noscript>Please enable javascript!</noscript>

    <div id="root"></div>

    <div id="ui-stack"></div>

    <div class="templates">
        <div class="ui-popup">
            <div class=ui-popup-overlay></div>
            <div class=ui-popup-outer>
                <div class="ui-popup-bg tl"></div>
                <div class="ui-popup-bg t"></div>
                <div class="ui-popup-bg tr"></div>
                <div class="ui-popup-bg l"></div>
                <div class="ui-popup-bg m"></div>
                <div class="ui-popup-bg r"></div>
                <div class="ui-popup-bg bl"></div>
                <div class="ui-popup-bg b"></div>
                <div class="ui-popup-bg br"></div>
                <div class=ui-popup-inner>
                </div>
            </div>
        </div>
        <img src="/static/popup.png" />
    </div>

    <img style="display:none;" id="loader_img" alt="" src="{{ static_url(title_img) }}">

    <script>
      window.use_cdn = {{ "true" if use_cdn else "false" }};
    </script>
    <script src="{{'//cdnjs.cloudflare.com/ajax/libs/require.js/2.1.11/require.min.js' if use_cdn else '/static/scripts/contrib/require.js'}}" data-main="{{ maybe_minified('scripts/app') }}"></script>
  </body>
</html>
